docker run -d \
  --name mysql-server \
  --network my-network \
  -e MYSQL_ROOT_PASSWORD=AAa123456 \
  -v ./mysql-local-test/data:/var/lib/mysql \
  -p 3306:3306 \
  mysql:8.0


docker run -it --rm \
  --network my-network \
  mysql:8.0 \
  mysql -h mysql-server -u root -p


docker run -i --rm \
  --network my-network \
  -v $(pwd):/sql \
  mysql:8.0 \
  mysql -h mysql-server -u root -pAAa123456 < your_file.sql
